<div class="container">
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">
            <h1 class="h3">Data for A specific taxi</h1>
        </div>
        <div class="col-3"></div>
    </div>
</div>


<div class="container-fluid">

    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">
            <table class="table">
                <tr>
                    <th>Location Name</th>
                    <th>Taxi Reg#</th>
                    <th>Taxi Region</th>
                    <th>Amount Per TRip</th>

                </tr>
                <form action="/specific" method="get">
                    
                    <tr>

                        <td> {{route_name}} </td>
                        <td> {{totIncome.trip_reg_number}} </td>
                        <td> {{taxiRegion.name}} </td>
                        <td> {{totIncome.sum}} </td>


                    </tr>
                    
                </form>
            </table>


        </div>
        <div class="col-3"></div>
    </div>


</div>
{{#each specificTaxi}}



{{/each}}